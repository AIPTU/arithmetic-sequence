{
  "extends": [
    "config:best-practices",
    ":disableDependencyDashboard"
  ],
  "labels": [
    "dependencies"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": [
        "minor",
        "patch"
      ],
      "automerge": true
    },
    {
      "matchUpdateTypes": [
        "major"
      ],
      "automerge": false
    }
  ],
  "automergeStrategy": "squash",
  "prHourlyLimit": 5,
  "prConcurrentLimit": 3
}